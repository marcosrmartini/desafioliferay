<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_2_0.dtd">

<service-builder dependency-injector="ds" package-path="com.liveray.client.service">
	<namespace>cl</namespace>

	<entity name="Cliente" table="client" local-service="true" remote-service="false">

		<!-- PK fields -->

		<column name="clienteId" primary="true" type="long"></column>

		<!-- Group instance -->

		<column name="planoId" type="long"></column>

		<!-- Audit fields -->

		<column name="nome" type="String"></column>
		<column name="rg" type="String"></column>
		<column name="nomeMae" type="String"></column>
		<column name="nomePai" type="String"></column>
		<column name="orgaoExpedidor" type="String"></column>
        <column name="estado" type="String"></column>
        <column name="dataExpedicao" type="Date"></column>
	</entity>
	
</service-builder>